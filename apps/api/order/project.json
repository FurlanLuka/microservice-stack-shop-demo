{
  "name": "api-order",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/api/order/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/api/order",
        "main": "apps/api/order/src/main.ts",
        "tsConfig": "apps/api/order/tsconfig.app.json",
        "assets": [
          "apps/api/order/src/assets"
        ],
        "webpackConfig": "apps/api/order/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "api-order:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "api-order:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/api/order/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/api/order/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "generate-migrations": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build api-order && node dist/apps/api-order/main generate-migrations"
      }
    }
  },
  "tags": [
    "scope:api:lib:util",
    "scope:api:lib:order"
  ]
}
